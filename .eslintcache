\r\n        else {\r\n            audioRef.current.play();\r\n        }\r\n        setIsPlaying(!isPlaying);\r\n    }\r\n\r\n    const dragHandler = (e) => {\r\n        audioRef.current.currentTime = e.target.value;\r\n        setSongInfo({ ...songInfo, currentTime: e.target.value });\r\n    }\r\n\r\n    //Function\r\n    const getTime = (time) => {\r\n        return Math.floor(time / 60) + ':' + ('0' + Math.floor(time % 60)).slice(-2);\r\n    }\r\n\r\n    const skipTrackHandler = async (direction) => {\r\n        //Find current song\r\n        let currentIndex = songs.findIndex((s) => s.id === currentSong.id);\r\n\r\n        //Skip track\r\n        if (direction === 'skip-forward') {\r\n            currentIndex++;\r\n            await setCurrentSong(songs[(currentIndex < songs.length ? currentIndex : 0)]);\r\n        }\r\n        else {\r\n            currentIndex--;\r\n            await setCurrentSong(songs[(currentIndex >= 0 ? currentIndex : songs.length - 1)]);\r\n        }\r\n\r\n        //Play if it's playing\r\n        playAudio(isPlaying, audioRef);\r\n\r\n    }\r\n\r\n    //Add styles\r\n    const trackAnim = {\r\n        transform: `translateX(${songInfo.animationPercentage}%)`\r\n    }\r\n\r\n\r\n    return (\r\n        <div className='player'>\r\n            <div className=\"time-control\">\r\n                <p>{getTime(songInfo.currentTime)}</p>\r\n                <div style={{ background: `linear-gradient(to right, ${currentSong.color[0]}, ${currentSong.color[1]})` }} className=\"track\">\r\n                    <input onChange={dragHandler} min={0} max={songInfo.duration || 0} value={songInfo.currentTime} type=\"range\" />\r\n                    <div className=\"animate-track\" style={trackAnim}></div>\r\n                </div>\r\n                <p>{getTime(songInfo.duration || 0)}</p>\r\n            </div>\r\n            <div className=\"play-control\">\r\n                <FontAwesomeIcon size='2x' className='skip-back' icon={faAngleLeft} onClick={() => { skipTrackHandler('skip-back') }} />\r\n                <FontAwesomeIcon size='2x' className='play' icon={(isPlaying ? faPause : faPlay)} onClick={playSongHandler} />\r\n                <FontAwesomeIcon size='2x' className='skip-forward' icon={faAngleRight} onClick={() => { skipTrackHandler('skip-forward') }} />\r\n            </div>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Player\r\n","C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\components\\Song.js",[],"C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\data.js",[],"C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\components\\Library.js",[],"C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\components\\LibrarySong.js",[],"C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\components\\Nav.js",[],"C:\\Users\\ariel\\source\\repos\\arielvergarautn\\music-app\\src\\util.js",[],{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1,"message":"48","line":26,"column":8,"nodeType":"49","endLine":26,"endColumn":21,"suggestions":"50"},"no-native-reassign",["51"],"no-negated-in-lhs",["52"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'setSongs' and 'songs'. Either include them or remove the dependency array. If 'setSongs' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["53"],"no-global-assign","no-unsafe-negation",{"desc":"54","fix":"55"},"Update the dependencies array to be: [currentSong, setSongs, songs]",{"range":"56","text":"57"},[844,857],"[currentSong, setSongs, songs]"]